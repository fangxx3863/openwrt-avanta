#!/bin/sh
#
# Copyright (C) 2012-2014 OpenWrt.org
#

. /lib/functions/uci-defaults.sh
. /lib/kirkwood.sh

board=$(kirkwood_board_name)

case "$board" in
"f660")
	ucidef_set_led_default "warn" "warn:red" "1"
	ucidef_set_led_default "voice1" "voice1:green" "1"
	ucidef_set_led_default "ponnetwork" "ponnetwork:green" "1"
	ucidef_set_led_default "ponport" "ponport:red" "1"
	ucidef_set_led_default "voice2" "voice2:green" "1"
	ucidef_set_led_default "wpsr" "wps:red" "1"
	ucidef_set_led_default "wpsg" "wps:green" "1"
	ucidef_set_led_default "usb" "usb:green" "1"
	;;
*)
	;;
esac

ucidef_commit_leds

exit 0
