+
+/dts-v1/;
+
+#include "avanta.dtsi"
+#include "avanta-6560.dtsi"
+
+/ {
+	model = "HQW HGG420N";
+	compatible = "hql,hgg420n", "marvell,kirkwood-88f6282", "marvell,kirkwood";
+
+	memory {
+		device_type = "memory";
+		reg = <0x00000000 0x8000000>;
+	};
+
+	chosen {
+		bootargs = "console=ttyS0,115200n8 earlyprintk";
+	};
+
+
+	mbus {
+		pcie-controller {
+			status = "okay";
+
+			pcie@1,0 {
+				status = "okay";
+			};
+
+			pcie@2,0 {
+				status = "okay";
+			};
+		};
+	};
+
+	ocp@f1000000 {
+/*
+		pinctrl: pinctrl@10000 {
+			pmx_led_white_health: pmx-led-white-health {
+				marvell,pins = "mpp7";
+				marvell,function = "gpo";
+			};
+			pmx_led_white_pulse: pmx-led-white-pulse {
+				marvell,pins = "mpp14";
+				marvell,function = "gpio";
+			};
+			pmx_btn_wps: pmx-btn-wps {
+				marvell,pins = "mpp47";
+				marvell,function = "gpio";
+			};
+			pmx_btn_reset: pmx-btn-reset {
+				marvell,pins = "mpp48";
+				marvell,function = "gpio";
+			};
+		};
+*/
+
+		rtc@10300 {
+			status = "disabled";
+		};
+
+		serial@12000 {
+			status = "okay";
+		};
+	};
+
+/*
+	gpio_keys {
+		compatible = "gpio-keys";
+		#address-cells = <1>;
+		#size-cells = <0>;
+		pinctrl-0 = < &pmx_btn_wps &pmx_btn_reset >;
+		pinctrl-names = "default";
+
+		button@15 {
+			label = "WPS Button";
+			linux,code = <KEY_WPS_BUTTON>;
+			gpios = <&gpio1 15 GPIO_ACTIVE_LOW>;
+		};
+
+		button@16 {
+			label = "Reset Button";
+			linux,code = <KEY_RESTART>;
+			gpios = <&gpio1 16 GPIO_ACTIVE_LOW>;
+		};
+	};
+*/
+
+	gpio-leds {
+		compatible = "gpio-leds";
+
+		green-ponnetwork {
+			label = "hgg420n:red:ponnetwork";
+			gpios = <&gpio1 10 GPIO_ACTIVE_LOW>;
+		};
+
+		green-ponport {
+			label = "hgg420n:green:ponport";
+			gpios = <&gpio1 12 GPIO_ACTIVE_LOW>;
+		};
+
+		green-voice1 {
+			label = "hgg420n:green:voice1";
+			gpios = <&gpio1 13 GPIO_ACTIVE_LOW>;
+		};
+
+		green-voice2 {
+			label = "hgg420n:green:voice2";
+			gpios = <&gpio1 14 GPIO_ACTIVE_LOW>;
+		};
+
+		green-network {
+			label = "hgg420n:green:network";
+			gpios = <&gpio1 15 GPIO_ACTIVE_LOW>;
+		};
+
+		green-wan {
+			label = "hgg420n:green:wan";
+			gpios = <&gpio1 17 GPIO_ACTIVE_LOW>;
+		};
+
+		green-usb {
+			label = "hgg420n:green:usb";
+			gpios = <&gpio1 25 GPIO_ACTIVE_LOW>;
+		};
+
+		green-power {
+			label = "hgg420n:green:power";
+			gpios = <&gpio1 26 GPIO_ACTIVE_HIGH>;
+		};
+
+		red-wps {
+			label = "hgg420n:red:wps";
+			gpios = <&gpio1 28 GPIO_ACTIVE_LOW>;
+		};
+
+		green-wps {
+			label = "hgg420n:green:wps";
+			gpios = <&gpio2 16 GPIO_ACTIVE_LOW>;
+		};
+	};
+
+	gpio_keys {
+		compatible = "gpio-keys";
+
+		button@8 {
+			label = "wps";
+			linux,code = <KEY_WPS_BUTTON>;
+			gpios = <&gpio2 8 GPIO_ACTIVE_LOW>;
+		};
+
+		button@9 {
+			label = "wireless";
+			linux,code = <KEY_WIMAX>;
+			gpios = <&gpio2 9 GPIO_ACTIVE_LOW>;
+		};
+	};
+
+	mvsw6171 {
+		compatible = "marvell,88e6171";
+		status = "okay";
+		reg = <0x8>;
+
+		mii-bus = <&mdio>;
+		cpu-port-0 = <4>;
+		cpu-port-1 = <5>;
+		is-indirect;
+	};
+};
+
+&nand {
+	status = "okay";
+/*
+	pinctrl-0 = <&pmx_nand_none>;
+	pinctrl-names = "default";
+*/
+
+	partition@0 {
+		label = "u-boot";
+		reg = <0 0x100000>;
+	};
+
+	partition@100000 {
+		label = "kernel";
+		reg = <0x100000 0x200000>;
+	};
+
+	partition@300000 {
+		label = "ubi";
+		reg = <0x300000 0x3d00000>;
+	};
+};
+
+&mdio {
+	status = "okay";
+};
+
+&eth0 {
+	status = "okay";
+	phy-mode = "gmii";
+
+	fixed-link {
+		speed = <1000>;
+		full-duplex;
+	};
+};
